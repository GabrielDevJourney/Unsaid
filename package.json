{
    "name": "unsaid",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build && npm run sentry:sourcemaps",
        "start": "next start",
        "lint": "biome check .",
        "lint:fix": "biome check . --write",
        "format": "biome format . --write",
        "typecheck": "tsc --noEmit",
        "test": "vitest",
        "test:run": "vitest run",
        "test:coverage": "vitest run --coverage",
        "commit": "cz",
        "seed:progress": "tsx scripts/seed-progress-test.ts",
        "sentry:sourcemaps": "sentry-cli sourcemaps inject --org unsaid-m2 --project javascript-nextjs .next && sentry-cli sourcemaps upload --org unsaid-m2 --project javascript-nextjs .next"
    },
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
    },
    "dependencies": {
        "@ai-sdk/anthropic": "^2.0.56",
        "@ai-sdk/openai": "^2.0.86",
        "@clerk/nextjs": "^6.36.2",
        "@react-email/components": "^1.0.3",
        "@react-email/render": "^2.0.1",
        "@sentry/cli": "^2.58.4",
        "@sentry/nextjs": "^10.32.1",
        "@supabase/supabase-js": "^2.87.1",
        "ai": "^5.0.113",
        "next": "16.0.10",
        "react": "19.2.1",
        "react-dom": "19.2.1",
        "react-email": "^5.0.8",
        "resend": "^6.6.0",
        "zod": "^4.1.13"
    },
    "devDependencies": {
        "@biomejs/biome": "2.2.0",
        "@commitlint/cli": "^20.2.0",
        "@commitlint/config-conventional": "^20.2.0",
        "@tailwindcss/postcss": "^4",
        "@testing-library/dom": "^10.4.1",
        "@testing-library/react": "^16.3.1",
        "@types/node": "^20",
        "@types/react": "^19",
        "@types/react-dom": "^19",
        "@vitejs/plugin-react": "^5.1.2",
        "commitizen": "^4.3.1",
        "cz-conventional-changelog": "^3.3.0",
        "dotenv": "^17.2.3",
        "jsdom": "^27.3.0",
        "lefthook": "^2.0.11",
        "supabase": "^2.67.1",
        "tailwindcss": "^4",
        "tsx": "^4.21.0",
        "typescript": "^5",
        "vitest": "^4.0.16"
    }
}
